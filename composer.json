{
    "name": "rogervila/laravel-legacy-ui",
    "description": "Legacy UI with Auth scaffolding for Laravel 8",
    "keywords": ["laravel", "ui", "legacy", "auth scaffolding"],
    "license": "MIT",
    "authors": [{
        "name": "Taylor Otwell",
        "email": "taylor@laravel.com"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^8.0",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\LegacyUi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\LegacyUi\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\LegacyUi\\UiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
